{
  "name": "@bit/bit.envs.bundlers.vue",
  "version": "2.6.21",
  "homepage": "https://bit.dev/bit/envs/bundlers/vue",
  "main": "dist/vue-bundler/index.js",
  "componentId": {
    "scope": "bit.envs",
    "name": "bundlers/vue",
    "version": "2.6.21"
  },
  "dependencies": {
    "vinyl": "^2.2.0",
    "memory-fs": "0.4.1",
    "webpack": "4.39.3",
    "@babel/preset-env": "7.6.0",
    "babel-loader": "8.0.6",
    "css-loader": "0.28.11",
    "less-loader": "5.0.0",
    "sass-loader": "6.0.7",
    "ts-loader": "6.2.0",
    "typescript": "3.6.3",
    "vue-loader": "15.7.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.10",
    "vue": "2.6.10",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@types/vinyl": "^2.0.3",
    "@types/node": "^12.7.4",
    "@types/mocha": "^5.2.7"
  },
  "peerDependencies": {},
  "license": "SEE LICENSE IN LICENSE",
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/typescript@3.1.31": {
          "rawConfig": {
            "compilerPath": "typescript/bin/tsc",
            "compilerArguments": [
              "-d"
            ],
            "compiledFileTypes": [
              ".ts",
              ".tsx"
            ],
            "configFileName": "tsconfig.json",
            "tsconfig": {
              "compilerOptions": {
                "outDir": "dist",
                "target": "ES5",
                "sourceMap": false,
                "moduleResolution": "Node",
                "esModuleInterop": true,
                "module": "CommonJS",
                "allowSyntheticDefaultImports": true,
                "resolveJsonModule": true,
                "declaration": true,
                "experimentalDecorators": true,
                "inlineSourceMap": false,
                "inlineSources": false,
                "jsx": "react",
                "rootDir": "./",
                "removeComments": true,
                "typeRoots": [
                  "./node_modules/@types"
                ],
                "importHelpers": false,
                "skipLibCheck": true
              },
              "include": [
                "./**/*"
              ],
              "exclude": [
                "node_modules",
                ".dependencies",
                "dist"
              ]
            },
            "development": false,
            "copyPolicy": {
              "ignorePatterns": [
                "package.json",
                "package-lock.json",
                "tsconfig.json"
              ],
              "disable": false
            },
            "useExperimentalCache": false
          },
          "options": {}
        }
      }
    },
    "overrides": {
      "devDependencies": {
        "@types/node": "+",
        "@types/mocha": "^5.2.7"
      }
    }
  }
}
